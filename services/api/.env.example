# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
# Se estiver usando Docker (docker-compose.yml), use as credenciais abaixo
# Se estiver usando MySQL local, ajuste conforme sua instalação
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=root
DB_NAME=expresso_consultoria_dev
DB_NAME_TEST=expresso_consultoria_test
DB_SSL=false

# JWT Configuration
# IMPORTANTE: Altere este secret em produção para um valor seguro
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=24h
